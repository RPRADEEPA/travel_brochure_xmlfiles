  <xs:element name="contact" type="xs:integer"/>
  <xs:element name="address" type="xs:string"/>
  <xs:element name="rating" type="xs:string"/>
  <xs:attribute name="cid" use="required" type="xs:string"/>
      </xs:sequence>
	  </xs:complexType>
	  </xs:element>
	  </xs:complexType>
	  </xs:element>
